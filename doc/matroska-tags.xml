<?xml version="1.0" encoding="ISO-8859-1"?>

<!--<!doctype Tags system "matroskatags.dtd">-->

<Tags>
  <Tag>
    <Targets>
      <!-- TrackUIDs and ChapterUIDs can occur more than once. -->
      <TrackUID>1</TrackUID>
      <TrackUID>2</TrackUID>
      <TrackUID>3</TrackUID>
      <TrackUID>4</TrackUID>
      <ChapterUID>2</ChapterUID>
      <ChapterUID>4</ChapterUID>
      <ChapterUID>6</ChapterUID>
      <ChapterUID>8</ChapterUID>
    </Targets>

    <General>
      <Subject>Subject</Subject>
      <Bibliography>Bibliography</Bibliography>
      <!-- All languages must be valid ISO639-2 codes. -->
      <Language>ger</Language>
      <Rating>QUJDREVG</Rating>
      <Encoder>Encoder</Encoder>
      <EncodeSettings>EncodeSettings</EncodeSettings>
      <File>File</File>
      <ArchivalLocation>ArchivalLocation</ArchivalLocation>
      <Keywords>Keywords</Keywords>
      <Mood>Mood</Mood>
      <RecordLocation>RecordLocation</RecordLocation>
      <Source>Source</Source>
      <SourceForm>SourceForm</SourceForm>
      <Product>Product</Product>
      <OriginalMediaType>OriginalMediaType</OriginalMediaType>
      <PlayCounter>1</PlayCounter>
      <!-- Popularimeter is actually the only element that is a signed integer
           and may be negative. -->
      <Popularimeter>-10</Popularimeter>
    </General>

    <Genres>
      <AudioGenre>AudioGenre</AudioGenre>
      <!-- VideoGenre is binary data base64 encoded. -->
      <VideoGenre>QUJDREVG</VideoGenre>
      <SubGenre>SubGenre</SubGenre>
    </Genres>

    <AudioSpecific>
      <!-- AudioEncryption is binary data base64 encoded. -->
      <AudioEncryption>QUJDREVG</AudioEncryption>
      <AudioGain>1.0</AudioGain>
      <AudioPeak>1.0</AudioPeak>
      <BPM>128</BPM>
      <Equalisation>QUJDREVG</Equalisation>
      <DiscTrack>1</DiscTrack>
      <SetPart>2</SetPart>
      <InitialKey>InitialKey</InitialKey>
      <OfficialAudioFileURL>OfficialAudioFileURL</OfficialAudioFileURL>
      <OfficialAudioSourceURL>OfficialAudioSourceURL</OfficialAudioSourceURL>
    </AudioSpecific>

    <ImageSpecific>
      <CaptureDPI>72</CaptureDPI>
      <CaptureLightness>QUJDREVG</CaptureLightness>
      <CapturePaletteSetting>256</CapturePaletteSetting>
      <CaptureSharpness>QUJDREVG</CaptureSharpness>
      <Cropped>Cropped</Cropped>
      <OriginalDimensions>OriginalDimensions</OriginalDimensions>
    </ImageSpecific>

    <MultiCommercial>
      <Commercial>
        <Type>1</Type>
        <Address>Address</Address>
        <URL>URL</URL>
        <Email>Email</Email>
        <MultiPrice>
          <Currency>Currency</Currency>
          <Amount>5</Amount>
          <!-- Dates are given in the extended ISO 8601 format. -->
          <PriceDate>2003-07-18T20:00:00+0200</PriceDate>
        </MultiPrice>
      </Commercial>
    </MultiCommercial>

    <MultiDate>
      <Date>
        <Type>2</Type>
        <Begin>2003-07-18T20:00:00+0100</Begin>
        <End>2003-07-18T20:00:00+0000</End>
      </Date>
    </MultiDate>

    <MultiEntity>
      <Entity>
        <Type>3</Type>
        <Name>Name</Name>
        <URL>URL</URL>
        <Email>Email</Email>
        <Address>Address</Address>
      </Entity>
    </MultiEntity>

    <MultiIdentifier>
      <Identifier>
        <Type>4</Type>
        <!-- Binary data can also be read from a file. This is indicated
             by the @ as the first character. The rest is the file name.
             The file itself is read as-is and NOT assumed to be Base64
             encoded. -->
        <Binary>@file.txt</Binary>
        <String>IdentifierString</String>
      </Identifier>
    </MultiIdentifier>

    <MultiLegal>
      <Legal>
        <Type>3</Type>
        <URL>URL</URL>
        <Address>Address</Address>
        <Content>(C) Content</Content>
      </Legal>
    </MultiLegal>

    <MultiTitle>
      <Title>
        <Type>2</Type>
        <Name>Name</Name>
        <SubTitle>SubTitle</SubTitle>
        <Edition>Edition</Edition>
        <Address>Address</Address>
        <URL>URL</URL>
        <Email>Email</Email>
        <Language>eng</Language>
      </Title>
      <!-- Let's test whether a complete second Title entry works. -->
      <Title>
        <Type>3</Type>
        <Name>Name2</Name>
        <SubTitle>SubTitle2</SubTitle>
        <Edition>Edition2</Edition>
        <Address>Address2</Address>
        <URL>URL2</URL>
        <Email>Email2</Email>
        <Language>fre</Language>
      </Title>
    </MultiTitle>

<!-- MultiComments don't work at the moment. -->
<!--     <MultiComment> -->
<!--       <Name>CommentName</Name> -->
<!--       <Comments>Comments</Comments> -->
<!--       <Language>ita</Language> -->
<!--     </MultiComment> -->

  </Tag>
</Tags>

